{
  "expo": {
    "name": "upai",
    "slug": "upai",
    "version": "3.0.3",
    "orientation": "portrait",
    "scheme": "upai",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.svg",
      "resizeMode": "contain",
      "backgroundColor": "#01C65C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "icon": "./assets/icon.svg",
      "supportsTablet": true,
      "bundleIdentifier": "com.example.upai",
      "buildNumber": "3.3.2",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["upai"],
        "NSCameraUsageDescription": "Приложение запрашивает доступ к камере для считывания qr кода и загрузки сделаных вами снимков",
        "NSPhotoLibraryUsageDescription": "Приложение запрашивет доступ к галереи для загрузки выбраной вами фотографии "
      }
    },
    "android": {
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.svg",
        "backgroundColor": "#FFFFFF"
      },

      "versionCode": 6,
      "googleServicesFile": "./google-services.json",
      "package": "com.up.upaii",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "app",
              "host": "upai://referal",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "notification": {
      "icon": "./assets/favicon.svg",
      "color": "#311b92",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "UPAI"
    },
    "web": {
      "favicon": "./assets/favicon.svg"
    }
  }
}
